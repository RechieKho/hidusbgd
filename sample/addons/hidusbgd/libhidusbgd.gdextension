[configuration]

entry_symbol = "hidusbgd_library_init"
compatibility_minimum = "4.1"

[libraries]

macos.debug = "./libhidusbgd.macos.template_debug.framework"
macos.release = "./libhidusbgd.macos.template_release.framework"
windows.debug.x86_32 = "./libhidusbgd.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "./libhidusbgd.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "./libhidusbgd.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "./libhidusbgd.windows.template_release.x86_64.dll"
windows.debug.arm64 = "./libhidusbgd.windows.template_debug.arm64.dll"
windows.release.arm64 = "./libhidusbgd.windows.template_release.arm64.dll"
linux.debug.x86_32 = "./libhidusbgd.linux.template_debug.x86_32.so"
linux.release.x86_32 = "./libhidusbgd.linux.template_release.x86_32.so"
linux.debug.x86_64 = "./libhidusbgd.linux.template_debug.x86_64.so"
linux.release.x86_64 = "./libhidusbgd.linux.template_release.x86_64.so"
linux.debug.arm32 = "./libhidusbgd.linux.template_debug.arm32.so"
linux.release.arm32 = "./libhidusbgd.linux.template_release.arm32.so"
linux.debug.arm64 = "./libhidusbgd.linux.template_debug.arm64.so"
linux.release.arm64 = "./libhidusbgd.linux.template_release.arm64.so"
linux.debug.rv64 = "./libhidusbgd.linux.template_debug.rv64.so"
linux.release.rv64 = "./libhidusbgd.linux.template_release.rv64.so"
android.debug.x86_64 = "./libhidusbgd.android.template_debug.x86_64.so"
android.release.x86_64 = "./libhidusbgd.android.template_release.x86_64.so"
android.debug.arm64 = "./libhidusbgd.android.template_debug.arm64.so"
android.release.arm64 = "./libhidusbgd.android.template_release.arm64.so"
ios.debug = "./libhidusbgd.ios.template_debug.xcframework"
ios.release = "./libhidusbgd.ios.template_release.xcframework"
web.debug.threads.wasm32 = "./libhidusbgd.web.template_debug.wasm32.wasm"
web.release.threads.wasm32 = "./libhidusbgd.web.template_release.wasm32.wasm"
web.debug.wasm32 = "./libhidusbgd.web.template_debug.wasm32.nothreads.wasm"
web.release.wasm32 = "./libhidusbgd.web.template_release.wasm32.nothreads.wasm"

[dependencies]
ios.debug = {
  "./libgodot-cpp.ios.template_debug.xcframework": ""
}
ios.release = {
  "./libgodot-cpp.ios.template_release.xcframework": ""
}